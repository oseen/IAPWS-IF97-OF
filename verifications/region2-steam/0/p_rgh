FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1.0e5;

boundaryField
{
    left
    {
        type            fixedFluxPressure;
        rho             rho;
        value           $internalField;
    }
    
    right
    {
        type            fixedValue;
        rho             rho;
        value           uniform 1.0e5;
    }

    "frontAndBack|topAndBottom"
    {
        type            empty;
    }
}

// ************************************************************************* //
